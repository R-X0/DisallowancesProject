adius) 0}ul.cards[data-orientation=media-end]>li>div.card-text{flex-grow:2}ul.cards[data-action=interactive]>li{box-shadow:var(--shadow);cursor:pointer;justify-content:start;position:relative;transition:all .3s ease-out}ul.cards[data-action=interactive]>li>img{margin-block-start:unset}ul.cards[data-action=interactive]>li>a{border-radius:var(--radius);color:currentcolor;display:block;height:100%;text-decoration:none;--outline-color:var(--brand-primary)}ul.cards[data-action=interactive]>li>div.card-text>:is(h1,h2,h3,h4,h5,h6)>a{color:inherit;text-decoration:none}ul.cards[data-action=interactive]>li>div.card-text>:is(h1,h2,h3,h4,h5,h6)>a:before{content:"";inset:0;position:absolute;z-index:3000}ul.cards[data-action=interactive]>li:focus-within{--outline-size:max(2px,0.08rem);--outline-style:solid;outline:var(--outline-size) var(--outline-style) var(--brand-primary);outline-offset:var(--outline-offset,var(--outline-size))}ul.cards[data-action=interactive]>li:focus-within a:focus{outline:none}ul.cards[data-action=interactive]>li:focus-within>img{filter:saturate(100%)}ul.cards[data-action=interactive]>li:focus,ul.cards[data-action=interactive]>li:hover{box-shadow:var(--shadow-focus);transform:scale(1.04)}ul.cards[data-action=interactive]>li:focus>img,ul.cards[data-action=interactive]>li:hover>img{filter:saturate(100%)}ul.cards[data-action=interactive]>li:active{transform:scale(1.02)}ul.cards[data-orientation^=media-]>li:only-child>img{max-inline-size:30%}ul.cards+ul.cards{margin-top:var(--gutter-gap)}ul.cards>li:has(.action:focus,.action:hover,.card-text:focus)>img{filter:saturate(100%)}@supports(display:grid){ul.cards{display:grid;grid-template-columns:repeat(auto-fit,minmax(18rem,1fr));grid-gap:var(--gutter-gap)}ul.cards>li{height:100%}ul.cards[data-orientation^=media-]{grid-auto-rows:minmax(20ch,auto);grid-template-columns:repeat(auto-fit,minmax(max(50ch,30vw),2fr))}ul.cards[data-orientation^=media-]:has(li:only-child){display:block;max-width:120ch}}

